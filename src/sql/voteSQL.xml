<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC '-//ibatis.apache.org//DTD Sql Map 2.0//EN' 'http://ibatis.apache.org/dtd/sql-map-2.dtd'>
<sqlMap namespace="vote">      

  <typeAlias alias="vdto" type="jackpot.DTO.voteDTO"/>

    <insert id="insertVote" parameterClass="vdto">
      
            insert into vote(v_num,jilmoon,v_start,emp_num,up_img,up_img2,monhang,monhang2) 
            values(vote_seq.nextval,#jilmoon#,sysdate,#emp_num#,#up_img#,#up_img2#,#monhang#,#monhang2#)
    </insert>
        
      
      
</sqlMap>



